key "Ctrl+Alt+t" flatpak run org.contourterminal.Contour
key "Ctrl+Alt+r" sudo reboot --force
key "Ctrl+Alt+s" dbus-send --system --print-reply --dest="org.freedesktop.login1" /org/freedesktop/login1 org.freedesktop.login1.Manager.Suspend boolean:true
key "Ctrl+Alt+L" i3lock -c 000000 -n
key "Ctrl+Alt+n" /usr/local/bin/lite-xl/lite-xl '/home/dkendall/OneDrive/Docume>
key "Ctrl+Alt+E" xfe
key "Ctrl+Alt+i" pavucontrol
key "Ctrl+Alt+v" /usr/bin/diodon
key "Print" flameshot gui
key "Shift+Print" simplescreenrecorder
key "Ctrl+Alt+m" flatpak run io.gitlab.zehkira.Monophony
